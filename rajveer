class Main {
  public static void main(String[] args) {
    System.out.println("Hello world!");
    int a,b; //decleration
    a=10; //initilization
      b=5;
      //int x=a%b;//modulo operator (remainder)
    int x=a/b;
    double y=a/b;
    System.out.println("Integer value is" +x);
    System.out.println("Integer value is" +y);
    System.out.println(b+b==a);
  }
}
